{
    "printWidth": 90,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "bracketSameLine": true,
    "singleQuote": false,
    "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"],
    "tailwindFunctions": ["clsx", "cn"],
    "overrides": [
        {
            "files": "*.php",
            "options": {
                "parser": "php",
                "plugins": ["@prettier/plugin-php"],
                "phpVersion": "8.2",
                "trailingCommaPHP": false,
                "singleQuote": true
            }
        }
    ]
}
